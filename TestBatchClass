@isTest
private class caseListUpdate {

static testmethod void test() {

// Create test case to be updated
// by the batch job.

Case[] caseList = new List();
for (Integer i=0;i<200;i++) {
Case m = new Case(Name = 'Case ' + i);
caseList.add(m);
}
insert caseList;

Test.startTest();
Batch_SendCaseCloseInfo  c = new Batch_SendCaseCloseInfo();
Database.executeBatch(c);
Test.stopTest();

// Verify case updated
Case[] caseUpdatedList = [SELECT Id,IsClosed  FROM Case];
System.assertcaseUpdatedList[0].IsClosed.equals(true));
}
}
